{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm ci",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "dist/vercel.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/vercel.js"
    }
  ],
  "crons": [
    {
      "path": "/api/jobs/auto-close",
      "schedule": "0 2 * * *"
    }
  ]
}
